<table class="table">
  <%
  @players.each do |player|
    %><%=
    "<tr id='#{player[0]}'>" +
    "<td class='player'><div class='name'> #{player[0].gsub("_"," ")} </div></td>" +
    "<td class='active'></td>"
    %><%
    217.times {%><%= "<td></td>" %><%}
    %><%= "<td class='goal'></td>" %><%
    20.times {%><%= "<td></td>" %><%}
    %><%= "</tr>"%><%
  end
  %>
</table>
<br>
<ul id="players_keys"></ul>
<div id="winner" class="hide">
  <div class="msg"></div>
  <button type="button" id="again">Jugar de nuevo</button>
</div>
<div id="statistics"></div>
<script src="/js/play.js"></script>
